student_scores = input("Input a list of student scores ").split()
length = 0
for position in range(0, len(student_scores)):
    student_scores[position] = int(student_scores[position])
    length = length+1

print(student_scores)
maxscore = 0
for score in student_scores:
    if score > maxscore:
        maxscore = score
print(f"The highest score in the class is: {maxscore}")
